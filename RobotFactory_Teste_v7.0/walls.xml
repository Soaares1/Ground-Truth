<?xml version="1.0" ?>

<obstacles> 
  <defines>
    <const name='v_length' value='4' />
	<const name='h_length' value='8' />
				
    <const name='wall_width' value='0.10' />
    <const name='wall_height' value='0.40' />
	
  </defines>

    <cuboid>
      <ID value='top_wall'/>
      <size x='h_length' y='wall_width' z='wall_height' />
      <pos x='0' y='v_length/2+0.05+0.03' z='wall_height/2'/>
      <rot_deg x='0' y='0' z='0'/>
      <color_rgb r='0' g='200' b='140'/>
    </cuboid>
	
	<cuboid>
      <ID value='bottom_wall'/>
      <size x='h_length' y='wall_width' z='wall_height' />
      <pos x='0' y='-v_length/2-0.05' z='wall_height/2'/>
      <rot_deg x='0' y='0' z='0'/>
      <color_rgb r='0' g='200' b='140'/>
    </cuboid>
	
    <cuboid>
      <ID value='right_wall'/>
      <size x='wall_width' y='v_length+0.03' z='wall_height' />
      <pos x='(h_length)/2+0.05' y='0.03/2' z='wall_height/2'/>
      <rot_deg x='0' y='0' z='0'/>
      <color_rgb r='0' g='200' b='140'/>
    </cuboid>

    <cuboid>
      <ID value='left_wall'/>
      <size x='wall_width' y='v_length+0.03' z='wall_height' />
      <pos x='-(h_length)/2-0.05' y='0.03/2' z='wall_height/2'/>
      <rot_deg x='0' y='0' z='0'/>
      <color_rgb r='0' g='200' b='140'/>
    </cuboid>


</obstacles>
   




