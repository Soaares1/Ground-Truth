<?xml version="1.0" ?>
<track>

  <defines>
    <const name='track_wall_dist' value='0.345' />
    <const name='track_width' value='0.05' />
    <const name='track_machine_slack' value='0.05' />
    <const name='ground' value='0.001' />
  </defines>

  <line>  <!-- outside horizontal lines -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x - track_width/2' y='max_field_y - track_wall_dist - track_width' z='ground' angle='0'/>
    <size width='track_width' length='max_field_x - track_wall_dist - (wharehouse_pos_x - track_width/2)'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- outside vertical lines -->
    <color rgb24='8F8F8F'/>
    <position x='-max_field_x + track_wall_dist' y='max_field_y - track_wall_dist' z='ground' angle='-90'/>
    <size width='track_width' length='2*(max_field_y - track_wall_dist)'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- middle vertical line -->
    <color rgb24='8F8F8F'/>
    <position x='-track_width/2' y='max_field_y - track_wall_dist' z='ground' angle='-90'/>
    <size width='track_width' length='2*(max_field_y - track_wall_dist)'/>
    <tag value='white'/>
  </line>

  <line>  <!-- feed M1/M8 -->
    <color rgb24='8F8F8F'/>
    <position x='-max_field_x + track_wall_dist' y='machine_width/2 - track_width/2' z='ground' angle='0'/>
    <size width='track_width' length='corridor_width - track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed M3/M6 -->
    <color rgb24='8F8F8F'/>
    <position x='-max_field_x + track_wall_dist' y='-machine_width/2 - track_width/2' z='ground' angle='0'/>
    <size width='track_width' length='corridor_width - track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed M5/M4 -->
    <color rgb24='8F8F8F'/>
    <position x='0' y='machine_width/2 - track_width/2' z='ground' angle='0'/>
    <size width='track_width' length='central_corridor_width/2 - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed M7/M2 -->
    <color rgb24='8F8F8F'/>
    <position x='0' y='-machine_width/2 - track_width/2' z='ground' angle='0'/>
    <size width='track_width' length='central_corridor_width/2 - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed W1 -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x + track_width/2' y='max_field_y - track_wall_dist' z='ground' angle='90'/>
    <size width='track_width' length='track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed W2 -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x + track_width/2 + machine_width' y='max_field_y - track_wall_dist' z='ground' angle='90'/>
    <size width='track_width' length='track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

    <line>  <!-- feed W3 -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x + track_width/2 + 2*machine_width' y='max_field_y - track_wall_dist' z='ground' angle='90'/>
    <size width='track_width' length='track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed W4 -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x + track_width/2 + 3*machine_width' y='max_field_y - track_wall_dist' z='ground' angle='90'/>
    <size width='track_width' length='track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <line>  <!-- feed W5 -->
    <color rgb24='8F8F8F'/>
    <position x='wharehouse_pos_x + track_width/2 + 4*machine_width' y='max_field_y - track_wall_dist' z='ground' angle='90'/>
    <size width='track_width' length='track_wall_dist - track_machine_slack'/>
    <tag value='white'/>
    <clone_hvflip/>
  </line>

  <!--
  <line>
    <color rgb24='8F8F8F'/>
    <position x='0' y='0' z='ground' angle='30'/>
    <size width='track_width' length='2*(track_width)'/>
    <clone_hvflip/>
  </line>
    <polygon>
    <color rgb24='8F8F8F'/>
    <vertice x='0' y='0' z='0'/>
    <vertice x='-0.1' y='-0.4' z='0'/>
    <vertice x='0.4' y='0.4' z='0'/>
  </polygon>
  <arc>
    <color rgb24='8F8F8F'/>
    <center x='0' y='0' z='ground'/>
    <radius inner='1.3' outer='1.4'/>
    <angle_deg start='0' stop='90' step='5'/>
  </arc>
  -->
</track>

